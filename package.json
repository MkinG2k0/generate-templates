{
  "name": "generate-templates",
  "version": "1.0.3",
  "description": "",
  "main": "./build/bin/main.js",
  "type": "module",
  "private": false,
  "contributors": [
    {
      "name": "Mut Kama",
      "email": "kamai122000@mail.ru"
    }
  ],
  "keywords": [
    "generate",
    "template",
    "script",
    "library",
    "ts",
    "js"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "exports": {
    ".": {
      "import": "./build/bin/main.js",
      "require": "./build/bin/main.js"
    }
  },
  "bin": {
    "gen-template": "./build/bin/main.js"
  },
  "scripts": {
    "dev": "npx nodemon",
    "test": "vitest",
    "lint": "eslint --fix ./src/**/*.ts*",
    "test:u": "vitest -u",
    "build": "rimraf ./build && npx tsc",
    "start": "npm run build && node build/bin/main.js /config/config.js ",
    "start:page": "npm run start page name: MyPage",
    "test-g:comp": "npm run start config/config.json comp MainComp DoubleComp",
    "test-g:page": "npm run start config/config.json page Main Auth",
    "test-g:page-comp": "npm run start config/config.json page-comp ./Main Form",
    "test-g:slice": "npm run start config/config.json slice Auth Product",
    "g:comp": "npm run start config/config.json comp Test",
    "g:page": "npm run start config/config.json page",
    "g:pc": "npm run start config/config.json page-comp",
    "g:slice": "npm run start config/config.json slice"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/fs-extra": "^9.0.13",
    "@types/node": "^18.11.10",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "eslint": "^8.29.0",
    "husky": "^8.0.2",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.0",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3"
  },
  "dependencies": {
    "@types/jest": "^29.5.1",
    "chalk": "^5.2.0",
    "directory-tree": "^3.4.0",
    "fs-extra": "^11.1.0",
    "vite": "^4.3.5",
    "vitest": "^0.31.0"
  }
}

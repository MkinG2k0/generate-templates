{
  "templates": {
    "page": {
      "template": "templates/page",
      "generate": "generated/Pages/"
    },
    "comp": {
      "template": "templates/comp",
      "generate": "generated/Components"
    },
    "page-comp": {
      "template": "templates/page",
      "generate": "generated/Pages/*/Components"
    },
    "slice": {
      "template": "templates/slice",
      "generate": "generated/Redux/Slices/",
      "replaceExt": [
        [
          "txt",
          "ts"
        ]
      ]
    }
  },
  "replaceName": "FileName",
  "replaceNameExt": [
    [
      "FileName.txt",
      "tsx"
    ]
  ]
}
